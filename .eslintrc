{
  "parser": "@babel/eslint-parser",
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": ["react-app", "airbnb", "prettier"],
  "rules": {
    "semi": ["error", "never"],
    "react/require-default-props": "off",
    "import/no-cycle": "off",
    "react/jsx-props-no-spreading": "off"
  }
}
